            "import requests\n",
            "import sys\n",
            "import time\n",
            "\n",
            "BASE_URL = \"http://localhost:8000\"\n",
            "\n",
            "def run_test():\n",
            "    print(\"Starting Test...\")\n",
            "    try:\n",
            "        # Health\n",
            "        requests.get(f\"{BASE_URL}/emotions\", timeout=2)\n",
            "        print(\"‚úÖ Server UP\")\n",
            "        \n",
            "        # Register\n",
            "        uname = f\"nb_user_{int(time.time())}\"\n",
            "        res = requests.post(f\"{BASE_URL}/register\", json={\"username\": uname, \"password\": \"test\", \"role\": \"client\"})\n",
            "        if res.status_code != 200: \n",
            "            print(f\"‚ùå Register Failed: {res.text}\"); return\n",
            "        uid = res.json()['user_id']\n",
            "        print(f\"‚úÖ Registered User ID: {uid}\")\n",
            "\n",
            "        # Login\n",
            "        res = requests.post(f\"{BASE_URL}/login\", json={\"username\": uname, \"password\": \"test\"})\n",
            "        if res.status_code != 200:\n",
            "            print(f\"‚ùå Login Failed: {res.text}\"); return\n",
            "        print(\"‚úÖ Login Successful\")\n",
            "        \n",
            "        print(\"\\nüéâ ALL TESTS PASSED\")\n",
            "    except Exception as e:\n",
            "        print(f\"‚ùå Error: {e}\")\n",
            "        print(\"Ensure server is running: python -m uvicorn main:app --reload\")\n",
            "\n",
            "run_test()"
        ]
    }
